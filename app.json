{
  "expo": {
    "name": "ShelfWay",
    "slug": "ShelfWay",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/Splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SplashW.png",
      "resizeMode": "contain",
      "backgroundColor": "#1976D2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SplashW.png",
        "backgroundColor": "#1976D2"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/Splash.png"
    },
    "plugins": [
      "expo-camera"
    ]
  }
}
